<!--  Jabber2 - jabberd JBOX configuration  -->
<jbox project='base'>
  <build>
    <make>
      <macro name='HEADERS'>
        <file>../jabberd.h</file>
      </macro>
      <macro name='OBJECTS'>
        <file>base_accept.o</file>
        <file>base_connect.o</file>
        <file>base_dynamic.o</file>
        <file>base_exec.o</file>
        <file>base_file.o</file>
        <file>base_format.o</file>
        <file>base_stderr.o</file>
        <file>base_stdout.o</file>
        <file>base_to.o</file>
      </macro>
      <compile>
 	<objectfile macro='OBJECTS'>
	  <dep><macro>HEADERS</macro></dep>
        </objectfile>
      </compile>
    </make>
  </build>
</jbox>
