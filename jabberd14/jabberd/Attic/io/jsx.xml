<!--  Jabber2 - jabberd/io JSX configuration  -->
<jsx project='io'>
  <build>
    <make>
      <macro name='HEADERS'>
        <file>../jabberd.h</file>
      </macro>
      <macro name='OBJECTS'>
        <file>base_connect.o</file>
        <file>base_exec.o</file>
        <file>base_stdout.o</file>
        <file>base_load.o</file>
        <file>base_accept.o</file>
      </macro>
      <compile>
 	<objectfile macro='OBJECTS'>
	  <dep><macro>HEADERS</macro></dep>
        </objectfile>
      </compile>
    </make>
  </build>
</jsx>
